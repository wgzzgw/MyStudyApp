<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example">
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <application
        android:name=".MyApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".mystudyapp.MainActivity" />
        <activity android:name=".loginandsign.SignAndLogin"
            android:theme="@style/BaseThemes">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activity.ResetPasswordActivity"
            android:theme="@style/BaseThemes"
            />
        <activity android:name=".activity.BaseActivity" />
        <activity android:name=".activity.ResetGenderActivity"
            android:theme="@style/BaseThemes"/>
        <activity android:name=".activity.ResetAreaActivity"
            android:theme="@style/BaseThemes"/>
        <activity android:name=".activity.PersonalActivity"
            android:theme="@style/BaseThemes"></activity>
        <activity android:name=".activity.NickSignActivity"
            android:theme="@style/BaseThemes"/>
    </application>

</manifest>